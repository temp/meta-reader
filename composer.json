{
    "name": "temp/meta-reader",
    "type": "library",
    "description": "meta reader",
    "keywords": ["meta", "media", "reader"],
    "license": "MIT",
    "authors": [
        {
            "name": "Stephan Wentz",
            "email": "stephan@wentz.it",
            "homepage": "http://www.wentz.it/"
        }
    ],
    "autoload": {
        "psr-4": {
            "Temp\\MetaReader\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Temp\\MetaReader\\Tests\\": "tests/"
        }
    },
    "require": {
        "php": ">=5.3.0",
        "psr/log": "~1.0.0"
    },
    "require-dev": {
        "phpunit/phpunit": "~4.5",
        "alchemy/phpexiftool": "~0.4",
        "php-ffmpeg/php-ffmpeg": "~0.5",
        "php-poppler/php-poppler": "~1.0",
        "temp/image-analyzer": "~1.0"
    },
    "suggest": {
        "phpexiftool/phpexiftool": "If you want to use the exif tool reader.",
        "php-ffmpeg/php-ffmpeg": "If you want to use the ffprobe reader.",
        "php-poppler/php-poppler": "If you want to use the pdfinfo reader.",
        "temp/image-analyzer": "If you want to use the image analyzer reader."
    },
    "extra": {
        "branch-alias": {
            "dev-master": "1.0-dev"
        }
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
